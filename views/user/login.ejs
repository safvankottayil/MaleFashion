<%- include('../layout/start') %>
<section class="vh-100">
    <img class=" gradient-custom vh-100 vw-100" src="img/login-img.jpg" alt="">

    <div class="container py-5 h-100">
      <div class="row d-flex justify-content-center align-items-center h-100">
        <div class="col-12 col-md-8 col-lg-6 col-xl-5 dark">
          <div class="card dark1 text-white" style="border-radius: 1rem;">
            <div class="card-body p-5 text-center">
  
              <div class="mb-md-5 mt-md-4 pb-3">
                <svg class="mb-4" width="195" height="23" viewBox="0 0 195 23" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                    <path d="M0 0H195V23H0V0Z" fill="url(#pattern0)"/>
                    <defs>
                    <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                    <use xlink:href="#image0_15_126" transform="scale(0.00512821 0.0434783)"/>
                    </pattern>
                    <image id="image0_15_126" width="195" height="23" xlink:href="data:image/png;base64,"/>
                    </defs>
                    </svg>
                    
                    
                <h2 class="fw-bold mb-2 text-uppercase">Login</h2>
                <!-- <p class="text-white-50 mb-5">Please enter your login and password!</p> -->
                    <form action="/login" method="post" id="loginform">
                <div class="form form-white mb-2">
                    <label class="form-label" for="typeEmailX">Email</label>
                  <input type="email" id="loginEmail" class="form-control form-control-lg" />
                  <small id="login-e-error" class="text-danger"></small>
                </div>
  
                <div class="form-outline form-white mb-2">
                    <label class="form-label" for="typePasswordX">Password</label>
                  <input type="password" id="loginPassword" class="form-control form-control-lg" />
                  <small id="login-p-error" class="text-danger"></small>

                </div>
  
                <p class="small mb-3 pb-lg-2"><a class="text-white-50" href="#!">Forgot password?</a></p>
  
                <button class="btn btn-outline-light btn-lg px-5" type="submit">Login</button>
              </form>
                <div class="d-flex justify-content-center text-center mt-2 pt-1">
                  <!-- <a href="#!" class="text-white"><i class="fab fa-facebook-f fa-lg"></i></a>
                  <a href="#!" class="text-white"><i class="fab fa-twitter fa-lg mx-4 px-2"></i></a>
                  <a href="#!" class="text-white"><i class="fab fa-google fa-lg"></i></a> -->
                </div>
  
              </div>
  
              <div>
                <p class="mb-0">Don't have an account? <a href="/register" class="text-white-50 fw-bold">Sign Up</a>
                </p>

              </div>
              <div>
                <p class="mb-0"><a href="/otp-login" class="text-white-50 fw-bold">Otp login</a>
                </p>
                
              </div>
  
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
 <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.5/jquery.validate.min.js"></script> 
  <script src="ajax/loginform.js"></script>

<%- include('../layout/end') %>