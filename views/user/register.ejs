                <h2 class="fw-bold mb-2 text-uppercase">Sign up</h2>
                <!-- <p class="text-white-50 mb-5">Please enter your login and password!</p> -->
  <form action="" method="" id="form"">

 
                <div class="form form-white mb-1">
                   
                  <input name="name" type="text" id="name" class="form-control form-control-lg" placeholder="Name"/>
                  <div class="err-div d-flex flex-end">
                    <small id="nameError" class=" ps-1 error text-danger"></small>
                </div>
                </div>
                <div class="form-outline form-white mb-2">
                  <input name="email" type="text" id="email" class="form-control form-control-lg " placeholder="Email" />
                  <div class="err-div d-flex flex-end">
                    <small class=" ps-1 error text-danger"></small>
                </div>
                </div>
                <div class="form-outline form-white mb-2">
                    <input name="mobile" type="text" id="mobile" class="form-control form-control-lg " placeholder="Mobile Number" />
                    <div class="err-div d-flex flex-end">
                        <small class=" ps-1 error text-danger"></small>
                    </div>
                  </div>
                  <div class="form-outline form-white mb-2">
                    <input name="password" type="password" id="password" class="form-control form-control-lg " placeholder="Password" />
                   <div class="err-div d-flex flex-end">
                        <small class=" ps-1 error text-danger"></small>
                    </div>
                  </div>
                  <div class="form-outline form-white mb-2">
                    <input name="repassword" type="password" id="repassword" class="form-control form-control-lg " placeholder="Repassword" />
                    <div class="err-div d-flex flex-end">
                        <small class="ps-1 error text-danger"></small>
                    </div>
                   
                  </div>
    
  
                
                <button id="submit" class="btn btn-outline-light btn-lg px-5" type="submit">Sign up</button>
  
            </form>
                <div class="d-flex justify-content-center text-center mt-2 pt-1">
                  <a href="#!" class="text-white"><i class="fab fa-facebook-f fa-lg"></i></a>
